{
  "name": "finance-mgn",
  "version": "1.0.0",
  "description": "Finance Management Tool (ETL)",
  "main": "./dist/app/index.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "format": "biome format --write",
    "lint": "biome lint",
    "lint:fix": "biome check --write",
    "start": "node dist/app/index.js",
    "start:prod": "yarn start --environment production --withLabeling true --actions fio",
    "cleanup:all": "yarn start --environment development --withLabeling false --actions none --cleanup all",
    "test:integration": "node ./src/tests/testRunner.mjs",
    "test": "vitest && node test:integration",
    "test:unit": "vitest -w=true",
    "typecheck": "tsc --noEmit -p config/tsconfig.json"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "googleapis": "^146.0.0",
    "imap-simple": "^5.1.0",
    "mailparser": "^3.7.2",
    "openai": "^4.85.4",
    "pdfjs-dist": "^4.10.38",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/imap-simple": "^4.2.9",
    "@types/yargs": "^17.0.33",
    "lefthook": "^1.11.6",
    "tsup": "^8.4.0"
  }
}
