{
  "name": "finance-mgn",
  "version": "1.0.0",
  "description": "Finance Management Tool (ETL)",
  "main": "./index.mjs",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write .",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "start": "node index.mjs",
    "start:prod": "node index.mjs --environment production --withLabeling true --actions fio",
    "cleanup:all": "node index.mjs --environment development --withLabeling false --actions none --cleanup all",
    "test:integration": "node ./src/tests/testRunner.mjs",
    "test": "vitest && node test:integration",
    "test:unit": "vitest -w=true",
    "prepare": "husky"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "googleapis": "^146.0.0",
    "imap-simple": "^5.1.0",
    "mailparser": "^3.7.2",
    "openai": "^4.85.4",
    "pdfjs-dist": "^4.10.38",
    "vitest": "^3.0.9",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "lefthook": "^1.11.6",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3"
  }
}
